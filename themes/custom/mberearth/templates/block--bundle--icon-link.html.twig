{% extends "block.html.twig" %}
{#
/**
 * @file
 * Theme override for a Focal Link.
 *
 * Available variables:
 * - Look in the block.html.twig file for available variables.
 */
#}
{% set label = FALSE %}
{% set class_box = FALSE %}

{% block content %}
  <a href="{{ content.field_link.0['#url'] }}" class="icon-link">
      <div class="icon-link__figure">
        {{ content.field_icon_image }}
      </div>
    <div class="icon-link__body">
      {#{{ devel_breakpoint() }}#}
      <strong>{{ content.field_link.0['#title'] }}</strong>
    </div>
  </a>
  {{ content|without('field_title', 'field_link', 'field_icon_image') }}
{% endblock %}
